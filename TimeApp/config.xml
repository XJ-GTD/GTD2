<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="905" id="cn.sh.com.xj.timeApp" ios-CFBundleVersion="22" version="1.9.4" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>冥王星</name>
    <description>管理好他/她的活动, 节约你的时间, 好好享受生活。</description>
    <author email="jy-zhang@zakj.info" href="http://ionicframework.com/">效吉软件</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="ScrollEnabled" value="false" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="KeyboardDisplayRequiresUserAction" value="false" />
    <preference name="BackgroundColor" value="0xff000000" />
    <preference name="StatusBarStyle" value="lightContent" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="resources/android/icon/drawable-ldpi-icon.png" />
        <icon density="mdpi" src="resources/android/icon/drawable-mdpi-icon.png" />
        <icon density="hdpi" src="resources/android/icon/drawable-hdpi-icon.png" />
        <icon density="xhdpi" src="resources/android/icon/drawable-xhdpi-icon.png" />
        <icon density="xxhdpi" src="resources/android/icon/drawable-xxhdpi-icon.png" />
        <icon density="xxxhdpi" src="resources/android/icon/drawable-xxxhdpi-icon.png" />
        <splash density="port-hdpi" src="resources/android/splash/drawable-port-hdpi-screen.jpg" />
        <splash density="port-xhdpi" src="resources/android/splash/drawable-port-xhdpi-screen.jpg" />
        <splash density="port-xxhdpi" src="resources/android/splash/drawable-port-xxhdpi-screen.jpg" />
        <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="UseSwiftLanguageVersion" value="4" />
        <preference name="WKSuspendInBackground" value="true" />
        <preference name="DisallowOverscroll" value="true" />
        <edit-config target="NSPhotoLibraryAddUsageDescription" file="*-Info.plist" mode="merge">
          <string>冥王星保存图片附件</string>
        </edit-config>
        <edit-config target="NSLocationWhenInUseUsageDescription" file="*-Info.plist" mode="merge">
          <string>冥王星添加图片附件</string>
        </edit-config>
        <edit-config target="NSPhotoLibraryUsageDescription" file="*-Info.plist" mode="merge">
          <string>冥王星选择图片附件</string>
        </edit-config>
        <edit-config target="NSCameraUsageDescription" file="*-Info.plist" mode="merge">
          <string>冥王星拍照保存附件</string>
        </edit-config>
    </platform>
    <plugin name="cordova-plugin-app-version" spec="~0.1.9" />
    <plugin name="cordova-plugin-camera" spec="~4.0.3" />
    <plugin name="cordova-plugin-chooser" spec="~1.2.5" />
    <plugin name="cordova-plugin-statusbar" spec="~2.4.3" />
    <plugin name="uk.co.workingedge.cordova.plugin.sqliteporter" spec="~1.1.1" />
    <plugin name="cordova-sqlite-storage" spec="~3.4.0" />
    <plugin name="cordova-plugin-whitelist" spec="~1.3.4" />
    <plugin name="com.telerik.plugins.nativepagetransitions" spec="~0.6.5" />
    <plugin name="cordova-plugin-nativeaudio" spec="~3.0.9" />
    <plugin name="cordova-plugin-vibration" spec="~3.1.1" />
    <plugin name="cordova-plugin-file-transfer" spec="~1.7.1" />
    <plugin name="cordova-plugin-telerik-imagepicker" spec="2.3.3">
      <variable name="PHOTO_LIBRARY_USAGE_DESCRIPTION" value="拍照上传活动补充" />
    </plugin>
    <plugin name="cordova-plugin-local-notification" spec="~0.9.0-beta.2" />
    <plugin name="cordova-plugin-ionic-webview" spec="~4.1.2" />
    <plugin name="cordova-plugin-splashscreen" spec="~5.0.3" />
    <plugin name="ionic-plugin-keyboard" spec="~2.2.1" />
    <plugin name="cordova-plugin-device" spec="~2.0.3" />
    <plugin name="cordova-plugin-console" spec="~1.1.0" />
    <plugin name="cordova-plugin-filepath" spec="~1.5.6" />
    <plugin name="cordova-plugin-file-opener2" spec="~2.2.1" />
    <plugin name="cordova-plugin-filechooser" spec="~1.2.0" />
    <plugin name="cordova-plugin-calendar" spec="~5.1.4" />
    <plugin name="cordova-plugin-advanced-http" spec="~2.2.0" />
    <plugin name="cordova-plugin-android-permissions" spec="~1.0.2" />
    <plugin name="cordova-plugin-background-mode" spec="~0.7.3" />
    <plugin name="cordova-plugin-contacts" spec="~3.0.1" />
    <plugin name="cordova-plugin-wechat" spec="~2.9.0">
        <variable name="WECHATAPPID" value="wx697676c0b8dd65f6" />
    </plugin>
    <plugin name="cordova-clipboard" spec="~1.3.0" />
    <plugin name="cordova-plugin-network-information" spec="~2.0.2" />
    <plugin name="cordova-plugin-screen-orientation" spec="~3.0.2" />
    <plugin name="jpush-phonegap-plugin" spec="~3.7.2">
        <variable name="APP_KEY" value="e8b1f201b4e0cc102f665238" />
        <variable name="CHANNEL" value="cn.sh.com.xj.timeApp" />
    </plugin>
    <plugin name="cordova-plugin-rabbitmq" spec="https://github.com/xiaoji-duan/cordova-plugin-rabbitmq.git" />
    <plugin name="cordova-plugin-geolocation" spec="~4.0.2">
        <variable name="GEOLOCATION_USAGE_DESCRIPTION" value="定位以获得更准确的天气预报" />
    </plugin>
    <plugin name="cordova-plugin-huaweipush" spec="../HuaweiPushPlugin" />
    <plugin name="cordova-plugin-BaiduSpeechAndTTS" spec="../BaiduSpeechAndTTS" />
    <plugin name="cordova-plugin-media" spec="~5.0.3" />
    <engine name="ios" spec="^4.4.0" />
    <engine name="browser" spec="^5.0.4" />
    <engine name="android" spec="~7.1.4" />
</widget>

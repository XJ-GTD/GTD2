<?xml version='1.0' encoding='utf-8'?>
<widget id="cn.sh.com.xj.timeApp" version="0.0.11" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>冥王星</name>
    <description>这是一个用H5混合开发的时间管理设计APP</description>
    <author email="jy-zhang@zakj.info" href="http://ionicframework.com/">效吉软件</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="ScrollEnabled" value="false" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="SplashScreenDelay" value="3000" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="resources/android/icon/drawable-icon.png" />
        <icon density="mdpi" src="resources/android/icon/drawable-icon.png" />
        <icon density="hdpi" src="resources/android/icon/drawable-icon.png" />
        <icon density="xhdpi" src="resources/android/icon/drawable-icon.png" />
        <icon density="xxhdpi" src="resources/android/icon/drawable-icon.png" />
        <icon density="xxxhdpi" src="resources/android/icon/drawable-icon.png" />
        <splash density="land-ldpi" src="resources/android/splash/drawable-screen.png" />
        <splash density="land-mdpi" src="resources/android/splash/drawable-screen.png" />
        <splash density="land-hdpi" src="resources/android/splash/drawable-screen.png" />
        <splash density="land-xhdpi" src="resources/android/splash/drawable-screen.png" />
        <splash density="land-xxhdpi" src="resources/android/splash/drawable-screen.png" />
        <splash density="land-xxxhdpi" src="resources/android/splash/drawable-screen.png" />
        <splash density="port-ldpi" src="resources/android/splash/drawable-screen.png" />
        <splash density="port-mdpi" src="resources/android/splash/drawable-screen.png" />
        <splash density="port-hdpi" src="resources/android/splash/drawable-screen.png" />
        <splash density="port-xhdpi" src="resources/android/splash/drawable-screen.png" />
        <splash density="port-xxhdpi" src="resources/android/splash/drawable-screen.png" />
        <splash density="port-xxxhdpi" src="resources/android/splash/drawable-screen.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="57" src="resources/ios/icon/icon.png" width="57" />
        <icon height="114" src="resources/ios/icon/icon.png" width="114" />
        <icon height="40" src="resources/ios/icon/icon.png" width="40" />
        <icon height="80" src="resources/ios/icon/icon.png" width="80" />
        <icon height="120" src="resources/ios/icon/icon.png" width="120" />
        <icon height="50" src="resources/ios/icon/icon.png" width="50" />
        <icon height="100" src="resources/ios/icon/icon.png" width="100" />
        <icon height="60" src="resources/ios/icon/icon.png" width="60" />
        <icon height="120" src="resources/ios/icon/icon.png" width="120" />
        <icon height="180" src="resources/ios/icon/icon.png" width="180" />
        <icon height="72" src="resources/ios/icon/icon.png" width="72" />
        <icon height="144" src="resources/ios/icon/icon.png" width="144" />
        <icon height="76" src="resources/ios/icon/icon.png" width="76" />
        <icon height="152" src="resources/ios/icon/icon.png" width="152" />
        <icon height="167" src="resources/ios/icon/icon.png" width="167" />
        <icon height="29" src="resources/ios/icon/icon.png" width="29" />
        <icon height="58" src="resources/ios/icon/icon.png" width="58" />
        <icon height="87" src="resources/ios/icon/icon.png" width="87" />
        <icon height="1024" src="resources/ios/icon/icon.png" width="1024" />
        <splash height="1136" src="resources/ios/splash/screen.png" width="640" />
        <splash height="1334" src="resources/ios/splash/screen.png" width="750" />
        <splash height="2208" src="resources/ios/splash/screen.png" width="1242" />
        <splash height="1242" src="resources/ios/splash/screen.png" width="2208" />
        <splash height="1536" src="resources/ios/splash/screen.png" width="2048" />
        <splash height="2048" src="resources/ios/splash/screen.png" width="2732" />
        <splash height="768" src="resources/ios/splash/screen.png" width="1024" />
        <splash height="2048" src="resources/ios/splash/screen.png" width="1536" />
        <splash height="2732" src="resources/ios/splash/screen.png" width="2048" />
        <splash height="1024" src="resources/ios/splash/screen.png" width="768" />
        <splash height="960" src="resources/ios/splash/screen.png" width="640" />
        <splash height="480" src="resources/ios/splash/screen.png" width="320" />
        <splash height="2732" src="resources/ios/splash/screen.png" width="2732" />
    </platform>
    <plugin name="org.nypr.cordova.wakeupplugin" spec="../xjalarmclock" />
    <plugin name="cordova-plugin-statusbar" spec="~2.4.2" />
    <plugin name="uk.co.workingedge.cordova.plugin.sqliteporter" spec="~1.1.0" />
    <plugin name="cordova-sqlite-storage" spec="~2.6.0" />
    <plugin name="cordova-plugin-whitelist" spec="~1.3.3" />
    <plugin name="com.telerik.plugins.nativepagetransitions" spec="~0.6.5" />
    <plugin name="cordova-plugin-nativeaudio" spec="~3.0.9" />
    <plugin name="cordova-plugin-vibration" spec="~3.1.0" />
    <plugin name="cordova-plugin-file-transfer" spec="~1.7.1" />
    <plugin name="cordova-plugin-local-notification" spec="~0.9.0-beta.2" />
    <plugin name="cordova-plugin-ionic-webview" spec="~2.3.1" />
    <plugin name="cordova-plugin-splashscreen" spec="~5.0.2" />
    <plugin name="ionic-plugin-keyboard" spec="~2.2.1" />
    <plugin name="cordova-plugin-device" spec="~2.0.2" />
    <plugin name="cordova-plugin-console" spec="~1.1.0" />
    <plugin name="cordova-plugin-file" spec="~6.0.1" />
    <plugin name="cordova-plugin-calendar" spec="~5.1.3" />
    <plugin name="cordova-plugin-advanced-http" spec="~2.0.2" />
    <plugin name="cordova-plugin-android-permissions" spec="~1.0.0" />
    <plugin name="cordova-plugin-background-mode" spec="~0.7.2" />
    <plugin name="cordova-plugin-BaiduSpeechAndTTS" spec="../BaiduSpeechAndTTS" />
    <plugin name="cordova-plugin-contacts" spec="3.0.1" />
    <plugin name="cordova-plugin-network-information" spec="2.0.1" />
    <engine name="ios" spec="^4.4.0" />
    <engine name="browser" spec="^5.0.4" />
    <engine name="android" spec="6.4.0" />
</widget>

{
    "name": "冥王星语音服务自然语言解析处理流",
    "trigger": "mwxing_inteligence_service_start",
    "parameters": [
        "userId",
        "content",
        "contentType"
    ],
    "follows": [
        {
            "name": "讯飞云AIUI语义分析",
            "trigger": "xfy",
            "parameters": [
                "userId;$.parent.parameters.userId",
                "dataType;$.parent.parameters.contentType",
                "content;$.parent.parameters.content"
            ],
            "outputs": [
                "xunfeiyun"
            ],
            "next": [
                {
                  "name": "数据清洗与转换",
                  "trigger": "acj",
                  "parameters": [
                      "datasouce;$.parent.outputs.xunfeiyun"
                  ],
                  "outputs": [
                      "cleaned"
                  ]
                }
            ]
        }
    ]
}
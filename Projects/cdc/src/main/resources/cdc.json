{
  "debug": true,
  "mongo.host": "mongodb",
  "mongo.port": 27017,
  "mongo.keepalive": true,
  "stomp.server.host": "sa-amq",
  "stomp.server.port": 5672,
  "amq.app.id": "cdc",
  "proxy": [
     {
       "url": "/aag/webhooks/:webhookowner/:version/:observer",
       "params": ["webhookowner", "version", "observer"],
       "headers": ["X-Hub-Signature"],
       "response": {
         "passthrough": true
       },
       "trigger": {
         "flowid": "mwxing_webhooks_api_proxy"
       }
     },
     {
       "url": "/aup/dologin",
       "path": "http://sa-aup:8080/aup/dologin",
       "params": [],
       "headers": ["lt", "pi", "pv", "di", "dt", "ai"],
       "response": {
         "passthrough": false
       },
       "trigger": {
         "flowid": "mwxing_api_proxy"
       }
     },
     {
       "url": "/aup/verifycode",
       "path": "http://sa-aup:8080/aup/verifycode",
       "params": [],
       "headers": ["lt", "pi", "pv", "di", "dt", "ai"],
       "response": {
         "passthrough": false
       },
       "trigger": {
         "flowid": "mwxing_api_proxy"
       }
     },
     {
       "url": "/aup/doregister",
       "path": "http://sa-aup:8080/aup/doregister",
       "params": [],
       "headers": ["lt", "pi", "pv", "di", "dt", "ai"],
       "response": {
         "passthrough": false
       },
       "trigger": {
         "flowid": "mwxing_api_proxy"
       }
     },
     {
       "url": "/aup/user/:unionid",
       "path": "http://sa-aup:8080/aup/user/#{unionid}",
       "params": ["unionid"],
       "headers": ["lt", "pi", "pv", "di", "dt", "ai"],
       "response": {
         "passthrough": false
       },
       "trigger": {
         "flowid": "mwxing_api_proxy"
       }
     },
     {
       "url": "/aup/user/:phoneno/userinfo",
       "path": "http://sa-aup:8080/aup/user/#{phoneno}/userinfo",
       "params": ["phoneno"],
       "headers": ["lt", "pi", "pv", "di", "dt", "ai"],
       "response": {

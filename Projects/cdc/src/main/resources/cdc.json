       "response": {
         "passthrough": false
       },
       "trigger": {
         "flowid": "mwxing_api_proxy"
       }
     },
     {
       "url": "/aup/user/:phoneno/avatar/json",
       "path": "http://sa-aup:8080/aup/user/#{phoneno}/avatar/json",
       "params": ["phoneno"],
       "headers": ["lt", "pi", "pv", "di", "dt", "ai"],
       "response": {
         "passthrough": false
       },
       "trigger": {
         "flowid": "mwxing_api_proxy"
       }
     }
   ]

{
  "_id": ObjectID("5d8d8455956dff0008e9ec66"),
    "name": "冥王星数据同步拉取请求处理流",
    "trigger": "mwxing_data_sync_pull_start",
    "parameters": [
      "mwxing",
      "mpn",
      "type",
      "d"
    ],
    "follows": [
        {
            "name": "数据拉取",
            "trigger": "sas",
            "parameters": [
                "type;pull",
                "header;$.parent.parameters.mwxing",
                "from;$.parent.parameters.mpn",
                "datatype;$.parent.parameters.type",
                "data;$.parent.parameters.d"
            ],
            "outputs": [
                "from",
                "header",
                "datas"
            ],
            "next": [
              {
                "name": "生成推送数据",
                "trigger": "acj",
                "parameters": [
                    "ruleid;MWXING_DATASYNC_V1_5",
                    "datasource;$.parent.outputs"
                ],
                "outputs": [
                    "cleaned"
                ],
              }
            ]
        }
    ]
}

{
  "_id": ObjectID("5d8d8455956dff0008e9ec66"),
    "name": "冥王星数据同步拉取请求处理流",
    "trigger": "mwxing_data_sync_pull_start",
    "parameters": [
      "mwxing",
      "mpn",
      "d"
    ],
    "follows": [
        {
            "name": "数据推送/拉取",
            "trigger": "sas",
            "parameters": [
                "type;pull",
                "header;$.parent.parameters.mwxing",
                "from;$.parent.parameters.mpn",
                "data;$.parent.parameters.d"
            ],
            "outputs": [
                "cleaned"
            ],
            "next": [
                {
                    "name": "冥王星通知服务",
                    "trigger": "ann",
                    "parameters": [
                        "announceTo;$.parent.outputs.cleaned.announceTo",
                        "announceType;$.parent.outputs.cleaned.announceType",
                        "announceContent;$.parent.outputs.cleaned.announceContent"
                    ],
                    "outputs": [
                        "result"
                    ]
                }
            ]
        }
    ]
}

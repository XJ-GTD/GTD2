{
  "_id": ObjectID("5cf64757a9f7cb0008b50a5c"),
    "name": "冥王星第三方WebHook事件处理流",
    "trigger": "mwxing_webhook_notification_start",
    "parameters": [
      "userId",
      "webhook",
      "event"
    ],
    "follows": [
        {
            "name": "github事件转换",
            "trigger": "acj",
            "parameters": [
                "ruleid;MWXING_GITHUB_EVENTS_GITHUB001_V1_4",
                "datasource;$.parent.parameters"
            ],
            "outputs": [
                "cleaned"
            ],
            "next": [
                {
                    "name": "冥王星通知服务",
                    "trigger": "ann",
                    "parameters": [
                        "announceTo;$.parent.outputs.cleaned.announceTo",
                        "announceType;$.parent.outputs.cleaned.announceType",
                        "announceContent;$.parent.outputs.cleaned.announceContent"
                    ],
                    "outputs": [
                        "result"
                    ]
                }
            ]
        }
    ]
}
